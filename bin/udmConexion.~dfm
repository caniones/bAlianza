  object zusTickets: TZUpdateSQL
    InsertSQL.Strings = (
      
        'INSERT INTO TICKETS (IDTICKET, FECHAHORA, IDADUANA, IDEXPORTADOR' +
        ', IDTRANSPORTE,'
      
        '    IDENTIFICADOR, IDMERCADERIA, IDVEHICULO, DNICHOFER, PESOTARA' +
        ', PESOBRUTO,'
      '    PESONETO, IDCONTENEDOR, IDESTADO, ENTRADA, SALIDA)'
      'VALUES ('
      '    :IDTICKET, '
      '    '#39'now'#39', '
      '    :IDADUANA, '
      '    :IDEXPORTADOR, '
      '    :IDTRANSPORTE, '
      '    :IDENTIFICADOR, '
      '    :IDMERCADERIA, '
      '    :IDVEHICULO, '
      '    :DNICHOFER, '
      '    :PESOTARA, '
      '    :PESOBRUTO, '
      '    :PESONETO, '
      '    :IDCONTENEDOR, '
      '    :IDESTADO,'
      '    '#39'now'#39', '
      '    '#39'now'#39
      ');')
    ModifySQL.Strings = (
      'UPDATE TICKETS a'
      'SET '
      '    a.IDADUANA = :IDADUANA, '
      '    a.IDEXPORTADOR = :IDEXPORTADOR, '
      '    a.IDTRANSPORTE = :IDTRANSPORTE, '
      '    a.IDENTIFICADOR = :IDENTIFICADOR, '
      '    a.IDMERCADERIA = :IDMERCADERIA, '
      '    a.IDVEHICULO = :IDVEHICULO, '
      '    a.DNICHOFER = :DNICHOFER, '
      '    a.PESOTARA = :PESOTARA, '
      '    a.PESOBRUTO = :PESOBRUTO, '
      '    a.PESONETO = :PESONETO, '
      '    a.IDCONTENEDOR = :IDCONTENEDOR, '
      '    a.IDESTADO = :IDESTADO,'
      '    a.ENTRADA = :ENTRADA,'
      '    a.SALIDA = :SALIDA'
      'WHERE'
      '    a.IDTICKET = :IDTICKET;')
    UseSequenceFieldForRefreshSQL = False
    Left = 40
    Top = 192
    ParamData = <
