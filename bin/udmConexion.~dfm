  object zqTickets: TZQuery
    Connection = zConnection
    SortedFields = 'IDTICKET'
    SortType = stDescending
    UpdateObject = zusTickets
    OnNewRecord = zqTicketsNewRecord
    SQL.Strings = (
      
        'SELECT r.IDTICKET, r.FECHAHORA, r.IDADUANA, A.CODIGOLOT AS CODIG' +
        'O_LOT, '
      '    r.IDEXPORTADOR, E.RAZONSOCIAL AS EXPORTADOR,  '
      
        '    r.IDTRANSPORTE, T.RAZONSOCIAL AS TRANSPORTE, C.NOMBRE||'#39' '#39'||' +
        'C.APELLIDO AS CHOFER,'
      
        '    r.IDENTIFICADOR, r.IDMERCADERIA, M.DESCRIPCION AS MERCADERIA' +
        ', '
      '    r.IDVEHICULO, r.DNICHOFER, r.PESOTARA,'
      
        '    r.PESOBRUTO, r.PESONETO, r.IDCONTENEDOR, r.IDESTADO, r.ENTRA' +
        'DA, r.SALIDA'
      'FROM TICKETS r'
      'INNER JOIN ADUANAS A ON A.IDADUANA = R.IDADUANA'
      'INNER JOIN EXPORTADORES E ON E.IDEXPORTADOR = R.IDEXPORTADOR'
      'INNER JOIN TRANSPORTES T ON T.IDTRANSPORTE = R.IDTRANSPORTE'
      'INNER JOIN MERCADERIAS M ON M.IDMERCADERIA = R.IDMERCADERIA'
      'INNER JOIN CHOFERES C ON C.NRODNI = R.DNICHOFER')
    Params = <>
    IndexFieldNames = 'IDTICKET Desc'
    Sequence = zsTickets
    SequenceField = 'IDTICKET'
    Left = 40
    Top = 96
